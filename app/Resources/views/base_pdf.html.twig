{% import "macros.html.twig" as mg %}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">

    {% block css %}
    <style>
        div,p{
            text-align: justify;
            font-size: 12.5px;
            line-height:12px;
        }

        *{
            font-family: 'Open Sans', sans-serif;
        }
    </style>
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
</body>
</html>
